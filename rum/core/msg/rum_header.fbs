namespace rum.msg;  // yeah it is not camel case, bite me

enum MsgType : byte{
    Interrupt,
    Message,
    ServiceRequest,
    ServiceResponse,
}

table ReqInfo{
    rep_topic:string;
    id:uint32;
}

table RepInfo{
    status:int8;
    id:uint32;
}

table MsgHeader{
    type:MsgType;
    // topic or srv
    name:string;
    protocal:string;

    // other fields depends on type
    request:ReqInfo;
    response:RepInfo;
}

root_type MsgHeader;